[
  {
    "name": "ip_mqttServer",
    "type": "ACInput",
    "value": "",
    "placeholder": "Example: 192.168.8.70",
    "label": "MQTT Server IP:",
    "pattern": "^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])$"
  },
  {
    "name": "containerID",
    "type": "ACInput",
    "value": "",
    "placeholder": "Example: 99-999-999",
    "label": "Container ID:",
    "pattern": "^([0][1-9]|[1-9][0-9])([-])([0][0][1-9]|[1-9][0-9][0-9]|[0-9][1-9][0-9])([-])([0][0][1-9]|[1-9][0-9][0-9]|[0-9][1-9][0-9])$"
  },
  {
    "name": "esp32Type",
    "type": "ACInput",
    "value": "",
    "placeholder": "front, center or back",
    "label": "What ESP32 are you setting?",
    "pattern": "(front|back|center)"
  },
  {
    "name": "mac_address",
    "type": "ACInput",
    "value": "",
    "placeholder": "FF:FF:FF:FF:FF:FF",
    "label": "What is the MAC address of your master ESP?",
    "pattern": "^([0-9A-F]{2}[:-]){5}([0-9A-F]{2})"
  }
]
