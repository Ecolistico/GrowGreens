[
  {
    "name": "ip_mqttServer",
    "type": "ACInput",
    "value": "",
    "placeholder": "Example: 192.168.8.70",
    "label": "MQTT Server IP:",
    "pattern": "^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])$"
  },
  {
    "name": "containerID",
    "type": "ACInput",
    "value": "",
    "placeholder": "Example: 99-999-999",
    "label": "Container ID:",
    "pattern": "^([0][1-9]|[1-9][0-9])([-])([0][0][1-9]|[1-9][0-9][0-9]|[0-9][1-9][0-9])([-])([0][0][0-9]|[1-9][0-9][0-9]|[0-9][1-9][0-9])$"
  },
  {
    "name": "esp32Type",
    "type": "ACInput",
    "value": "",
    "placeholder": "germination",
    "label": "What ESP32 are you setting?",
    "pattern": "(germination)"
  },
  {
    "name": "esp32Floor",
    "type": "ACInput",
    "value": "",
    "placeholder": "Example: 9",
    "label": "ESP32 Floor:",
    "pattern": "^([0-9])$"
  }
]
